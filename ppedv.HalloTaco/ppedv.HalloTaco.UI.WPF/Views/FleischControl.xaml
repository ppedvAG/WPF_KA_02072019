<UserControl x:Class="ppedv.HalloTaco.UI.WPF.Views.FleischControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ppedv.HalloTaco.UI.WPF.Views"
             mc:Ignorable="d" 
             d:DesignHeight="32" d:DesignWidth="60">
    <DockPanel>
        <Button Click="PrevClick" Content="&lt;" DockPanel.Dock="Left"/>
        <Button Click="NextClick" Content="&gt;" DockPanel.Dock="Right"/>
        <Image Name="img" Stretch="None" Width="32" Height="32"/>
    </DockPanel>
</UserControl>
